{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tribe/slider-container",
	"version": "0.1.0",
	"title": "Slider Container",
	"category": "theme",
	"icon": "images-alt2",
	"description": "A container block for a Swiper slider.",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": false
		}
